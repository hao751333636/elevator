            android:textColor="@color/white"
            app:layout_constraintTop_toBottomOf="@+id/view12"
            app:sCornersRadius="10dp"
            android:layout_marginTop="10dp"
            app:sShapeType="rectangle"
            app:sSolidColor="#157efb" />

